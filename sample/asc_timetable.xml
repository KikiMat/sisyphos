<timetable importtype="database" options="idprefix:MyApp" displayname="MyApp sample XML configuration" displaycountries="gr">
   <teachers options="import:disable,canadd,canremove,canupdate,primarytt,silent" columns="id,name,short">
   </teachers>
   <classes options="import:disable,canadd,canremove,canupdate,primarytt,silent" columns="id,name">
   </classes>
   <subjects options="import:disable,canadd,canremove,canupdate,primarytt,silent" columns="id,name,short">
   </subjects>
   <classrooms options="import:disable,canadd,canremove,canupdate,primarytt,silent" columns="id,name,short">
   </classrooms>
   <groups options="import:disable,canadd,canremove,canupdate,primarytt,silent" columns="id,name,classid,entireclass">
   </groups>
   <cards options="import:disable,canadd,canremove,canupdate,primarytt,silent" columns="day,period,classids,groupids,subjectid,teacherids,classroomid">
   </cards>
</timetable>